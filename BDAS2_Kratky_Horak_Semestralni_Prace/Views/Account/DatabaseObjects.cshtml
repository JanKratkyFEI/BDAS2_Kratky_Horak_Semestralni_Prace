@model List<DbObject>

<h2>Výpis databázových objektů</h2>

<div class="scrollable">
<table class="table">
    <thead>
        <tr>
            <th>Typ objektu</th>
            <th>Název objektu</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.ObjectType</td>
                <td>@obj.ObjectName</td>
            </tr>
        }
    </tbody>
</table>
 </div>

<a asp-action="Profile" class="btn btn-secondary">Zpět na profil</a>

