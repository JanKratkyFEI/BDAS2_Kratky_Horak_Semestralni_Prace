@model YourNamespace.Models.Predmet

@{
    ViewData["Title"] = "Detail předmětu";
}

<h2>Detail předmětu</h2>

<div>
    <h4>@Model.Nazev</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Popis</dt>
        <dd class="col-sm-10">@Model.Popis</dd>

        <dt class="col-sm-2">Typ</dt>
        <dd class="col-sm-10">@Model.Typ</dd>
    </dl>
</div>

<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Upravit</a>
<a asp-action="Index" class="btn btn-secondary">Zpět na seznam</a>

